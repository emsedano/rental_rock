<p id="notice"><%= notice %></p>

<h1>Listing Billing Data</h1>

<table>
  <thead>
    <tr>
      <th>Fiscal person</th>
      <th>Rfc</th>
      <th>Fiscal name</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>Neiborghood</th>
      <th>County</th>
      <th></th>
      <th>Country</th>
      <th>Zipcode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @billing_data.each do |billing_datum| %>
      <tr>
        <td><%= billing_datum.fiscal_person %></td>
        <td><%= billing_datum.rfc %></td>
        <td><%= billing_datum.fiscal_name %></td>
        <td><%= billing_datum.address1 %></td>
        <td><%= billing_datum.address2 %></td>
        <td><%= billing_datum.neiborghood %></td>
        <td><%= billing_datum.county %></td>
        <td><%= billing_datum. %></td>
        <td><%= billing_datum.country_id %></td>
        <td><%= billing_datum.zipcode %></td>
        <td><%= link_to 'Show', billing_datum %></td>
        <td><%= link_to 'Edit', edit_billing_datum_path(billing_datum) %></td>
        <td><%= link_to 'Destroy', billing_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

