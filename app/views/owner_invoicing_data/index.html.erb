<p id="notice"><%= notice %></p>

<h1>Listing Owner Invoicing Data</h1>

<table>
  <thead>
    <tr>
      <th>Owner</th>
      <th>Fiscal person</th>
      <th>Rfc</th>
      <th>Fiscal name</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>Neiborghood</th>
      <th>County</th>
      <th></th>
      <th>Country</th>
      <th>Zipcode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @owner_invoicing_data.each do |owner_invoicing_datum| %>
      <tr>
        <td><%= owner_invoicing_datum.owner_id %></td>
        <td><%= owner_invoicing_datum.fiscal_person %></td>
        <td><%= owner_invoicing_datum.rfc %></td>
        <td><%= owner_invoicing_datum.fiscal_name %></td>
        <td><%= owner_invoicing_datum.address1 %></td>
        <td><%= owner_invoicing_datum.address2 %></td>
        <td><%= owner_invoicing_datum.neiborghood %></td>
        <td><%= owner_invoicing_datum.county %></td>
        <td><%= owner_invoicing_datum. %></td>
        <td><%= owner_invoicing_datum.country_id %></td>
        <td><%= owner_invoicing_datum.zipcode %></td>
        <td><%= link_to 'Show', owner_invoicing_datum %></td>
        <td><%= link_to 'Edit', edit_owner_invoicing_datum_path(owner_invoicing_datum) %></td>
        <td><%= link_to 'Destroy', owner_invoicing_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

